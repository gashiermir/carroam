<!DOCTYPE html>
<html>
<head>
  <title>Modelle</title>
</head>
<body>
  <h1>Liste aller Modelle</h1>
  <a href="/modell/new">Neues Modell erstellen</a>
  <ul>
    <% modelle.forEach(function(modell) { %>
      <li>
        <a href="/modell/<%= modell.id %>">
          <%= modell.name %> (<%= modell.baujahr %>) - <%= modell.beschreibung %>
        </a>
        <a href="/modell/<%= modell.id %>/edit">Bearbeiten</a>
        <form action="/modell/<%= modell.id %>/delete" method="POST" style="display:inline;">
          <button type="submit">LÃ¶schen</button>
        </form>
      </li>
    <% }); %>
  </ul>
</body>
</html>

  